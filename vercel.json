{
    "version": 2,
    "builds": [
        {
            "src": "manage.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles_build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/manage.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "DJANGO_SETTINGS_MODULE": "dsiplay.settings"
    }
}